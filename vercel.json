{
    "cleanUrls": true,
    "trailingSlash": false,
    "redirects": [
        {
            "source": "/(www\\.)?tilqai-site\\.vercel\\.app(.*)",
            "destination": "https://tilqai.om$2",
            "permanent": true
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}